{
  "name": "@changerawr/react",
  "version": "0.1.0",
  "description": "Headless React SDK for Changerawr - changelog management system",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "npm run build",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@types/node": "^20.9.4",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^3.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/changerawr/react.git"
  },
  "keywords": [
    "changelog",
    "react",
    "headless",
    "changerawr",
    "sdk"
  ],
  "author": "Changerawr Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/changerawr/react/issues"
  },
  "homepage": "https://github.com/changerawr/react#readme"
}
